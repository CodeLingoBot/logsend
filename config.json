{
    "groups": [
        {
            "mask": "bee.[0-9]+.log$",
            "rules": [
                {
                    "columns": [
                        [
                            "gate"
                        ],
                        [
                            "exec_time",
                            "float"
                        ]
                    ],
                    "name": "gate_response",
                    "regexp": "chain_processor.+ .+MIRROR] ([a-zA-Z0-9_]+)_gate -> (.+)"
                },
                {
                    "columns": [
                        [
                            "code",
                            "int"
                        ],
                        [
                            "chains"
                        ]
                    ],
                    "name": "chains",
                    "regexp": "web.+] ([0-9]+) .+ /adaptors/chains/([a-zA-Z0-9_]+)"
                }
            ]
        },
        {
            "mask": "search.log",
            "rules": [
                {
                    "columns": [
                        [
                            "search",
                            "int"
                        ]
                    ],
                    "name": "keys_count",
                    "regexp": "metric dbcount ([0-9]+)$"
                },
                {
                    "columns": [
                        [
                            "search_prepare_proposals",
                            "int",
                            "DurationToMillisecond"
                        ]
                    ],
                    "name": "db",
                    "regexp": "PreapreProposals took (.+)$"
                }
            ]
        },
        {
            "mask": "map.log",
            "rules": [
                {
                    "columns": [
                        [
                            "map",
                            "int"
                        ]
                    ],
                    "name": "keys_count",
                    "regexp": "metric dbcount ([0-9]+)$"
                },
                {
                    "columns": [
                        [
                            "map_prepare_proposals",
                            "int",
                            "DurationToMillisecond"
                        ]
                    ],
                    "name": "db",
                    "regexp": "PreapreProposals took (.+)$"
                },
                {
                    "columns": [
                        [
                            "map_save_proposals",
                            "int",
                            "DurationToMillisecond"
                        ]
                    ],
                    "name": "db",
                    "regexp": "SaveProposalsForMap took (.+)$"
                }
            ]
        }
    ]
}
